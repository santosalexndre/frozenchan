    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="styles/frozen-theme.css">
    <title>Document</title>
</head>
<body>
    <h1> catalogo kk </h1>
    <% for (let thread of threads) { %>
        <h1>id: <%= thread.id %></h1>
